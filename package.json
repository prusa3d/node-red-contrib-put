{
  "name": "node-red-contrib-put",
  "version": "1.0.0",
  "description": "Collection of Node-RED nodes to interact with PUT",
  "dependencies": {
    "serialport": "^9.1.0",
    "mqtt": "^4.2.6"
  },
  "license": "Apache-2.0",
  "keywords": ["node-red", "put"],
  "node-red": {
    "version": ">=0.18.0",
    "nodes": {
      "put-ad-chans": "@put/ad-chans/ad-chans.js",
      "put-analog-read": "@put/analog-read/analog-read.js",
      "put-arduino": "@put/arduino/arduino.js",
      "put-beep": "@put/beep/beep.js",
      "put-command": "@put/command/command.js",
      "put-digital-output": "@put/digital-output/digital-output.js",
      "put-digital-read": "@put/digital-read/digital-read.js",
      "put-error-generator": "@put/error-generator/put-errors.js",
      "put-lcd": "@put/lcd/lcd.js",
      "put-lid-status": "@put/lid-status/lid-status.js",
      "put-lid-watchdog": "@put/lid-watchdog/lid-watchdog.js",
      "put-mqtt-command": "@put/mqtt-command/mqtt-command.js",
      "put-mqtt-debug": "@put/mqtt-debug/mqtt-debug.js",
      "put-power-control": "@put/power-control/power-control.js",
      "put-status-led": "@put/status-led/status-led.js",
      "put-test-report": "@put/test-report/test-report.js",
      "put-value-limit": "@put/value-limit/value-limit.js"
    }
  },
  "engines": {"node" : ">=8.6.0"}
}
